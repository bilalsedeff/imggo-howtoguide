# ImgGo API Configuration
# Get your API key from: https://img-go.com/settings/api-keys
IMGGO_API_KEY=your_imggo_api_key_here

# ImgGo Pattern IDs
# Create patterns at: https://img-go.com/patterns
INVOICE_PATTERN_ID=pat_invoice_json
SHELF_AUDIT_PATTERN_ID=pat_shelf_audit
RECEIPT_PATTERN_ID=pat_receipt_expense
DOC_CLASSIFICATION_PATTERN_ID=pat_doc_classification
PARKING_PATTERN_ID=pat_parking_lpr
CONSTRUCTION_PATTERN_ID=pat_construction_yaml
MEDICAL_PRESCRIPTION_PATTERN_ID=pat_prescription_text
KYC_PATTERN_ID=pat_kyc_verification
INVENTORY_PATTERN_ID=pat_inventory_csv

# Database Configuration
# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=imggo_data
DB_USER=postgres
DB_PASSWORD=your_db_password

# MySQL (alternative)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=imggo_data
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password

# MongoDB (alternative)
MONGODB_URI=mongodb://localhost:27017/imggo_data

# Third-Party Integrations

# Accounting Systems
QUICKBOOKS_CLIENT_ID=your_quickbooks_client_id
QUICKBOOKS_CLIENT_SECRET=your_quickbooks_client_secret
QUICKBOOKS_REFRESH_TOKEN=your_quickbooks_refresh_token
XERO_CLIENT_ID=your_xero_client_id
XERO_CLIENT_SECRET=your_xero_client_secret

# Expense Management
EXPENSIFY_API_KEY=your_expensify_api_key
EXPENSIFY_USER_ID=your_expensify_user_id
EXPENSIFY_USER_SECRET=your_expensify_secret

# CRM Systems
SALESFORCE_CLIENT_ID=your_salesforce_client_id
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret
SALESFORCE_USERNAME=your_salesforce_username
SALESFORCE_PASSWORD=your_salesforce_password
SALESFORCE_SECURITY_TOKEN=your_salesforce_token

# Cloud Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your_s3_bucket

GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
